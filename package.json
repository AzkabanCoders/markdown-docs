{
  "name": "reactdocs",
  "version": "1.8.1",
  "description": "A web document tool basead in markdown files data. [react + webpack + es2015 (babel)]",
  "main": "bin/index.js",
  "author": "Klayton Faria <klayton.souza@gmail.com>",
  "license": "ISC",
  "bin": {
    "reactdocs": "./bin/index"
  },
  "scripts": {
    "parseMarkdown": "node lib/build",
    "start": "node ./lib/reactdocs  --config ./reactdocs.json && webpack-dev-server --content-base app/  -w --port 3000 --progress --quiet --watch",
    "server": "node ./lib/reactdocs  --config ./reactdocs.json --compile --server",
    "build": "node ./lib/reactdocs  --config ./reactdocs.json --compile"
  },
  "parseMarkdown": {
    "watch": {
      "patterns": [
        "**/*"
      ],
      "extensions": "md"
    }
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "fs-sync": "^1.0.2",
    "jsonfile": "^2.3.1",
    "metalsmith": "^2.2.0",
    "metalsmith-collections": "^0.7.0",
    "metalsmith-filter": "^1.0.2",
    "metalsmith-markdown": "^0.2.1",
    "metalsmith-prompt": "0.0.8",
    "node-sass": "^3.9.3",
    "raw-loader": "^0.5.1",
    "react-svg-inline": "^1.2.0",
    "sass-loader": "^4.0.2",
    "webpack": "^1.13.2"
  },
  "dependencies": {
    "alt": "^0.18.6",
    "cli": "^1.0.0",
    "express": "^4.14.0",
    "history": "^2.1.2",
    "http": "0.0.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-highlight": "^0.9.0",
    "react-router": "^2.7.0",
    "restful.js": "^0.9.6"
  }
}
