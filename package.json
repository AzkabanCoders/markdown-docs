{
  "name": "reactdocs",
  "version": "1.7.0",
  "description": "A web document tool basead in markdown files data. [react + webpack + es2015 (babel)]",
  "main": "bin/index.js",
  "author": "Klayton Faria <klayton.souza@gmail.com>",
  "license": "ISC",
  "bin": {
    "reactdocs": "./bin/index"
  },
  "scripts": {
    "parseMarkdown": "node lib/build",
    "start":  "node ./lib/reactdocs  --config ./reactdocs.json && webpack-dev-server --content-base app/  -w --port 3000 --progress --quiet --watch",
    "server": "node ./lib/reactdocs  --config ./reactdocs.json --compile --server",
    "build":  "node ./lib/reactdocs  --config ./reactdocs.json --compile"
  },
  "parseMarkdown": {
  "watch": {
      "patterns": [
        "**/*"
      ],
      "extensions": "md"
    }
  },
  "devDependencies": {
    "fs-sync": "^1.0.2",
    "jsonfile": "^2.2.3",
    "metalsmith": "^2.1.0",
    "metalsmith-collections": "^0.7.0",
    "metalsmith-filter": "^1.0.2",
    "metalsmith-markdown": "^0.2.1",
    "metalsmith-prompt": "0.0.8",
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "extract-text-webpack-plugin": "^0.9.1",
    "react-svg-inline": "^1.1.0",
    "node-sass": "^3.4.2",
    "file-loader": "^0.8.5",
    "raw-loader": "^0.5.1",
    "css-loader": "^0.23.1",
    "sass-loader": "^3.1.2",
    "webpack": "^1.12.10"
  },
  "dependencies": {
    "alt": "^0.18.1",
    "cli": "^0.11.1",
    "express": "^4.13.3",
    "history": "^1.17.0",
    "http": "0.0.0",
    "react": "^0.14.5",
    "react-dom": "^0.14.6",
    "react-highlight": "^0.7.0",
    "react-router": "^1.0.3",
    "restful.js": "^0.9.6"
  }
}
